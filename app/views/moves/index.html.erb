<h1>Listing albums</h1>



<% @moves.each do |move| %>
  <tr>
    <td><%= move.move %></td>
    <td><%= link_to 'Show', [move.game, move] %></td>
    
    <td><%= link_to 'Destroy', [move.game, move], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

